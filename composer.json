{
    "name": "toddstoker/keap-sdk",
    "description": "Keap SDK for accessing Keap's Rest v1 and v2 API endpoints",
    "type": "library",
    "require": {
        "php": "^8.4",
        "saloonphp/saloon": "^3.0",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "psy/psysh": "^v0.12.18",
        "laravel/pint": "^1.27.0",
        "spatie/ray": "^1.44.0",
        "symfony/var-dumper": "^8.1.x-dev",
        "phpstan/phpstan": "^2.1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Toddstoker\\KeapSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "test": [
            "@test:lint",
            "@test:types"
        ],
        "test:lint": "pint --parallel --test",
        "test:types": "phpstan analyse",
        "pint": "pint --parallel",
        "phpstan": "phpstan analyse"
    },
    "extra": {
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
